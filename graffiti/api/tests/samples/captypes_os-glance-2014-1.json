  {
    "name": "VMwareComputeDriver",
    "namespace": "OS:GLANCE:2014-1",
    "description": "",
    "derived_from": {
      "name": "ComputeDriver",
      "namespace": "OS:GLANCE:2014-1"
    },
    "properties": {
      "vmware_adaptertype": {
        "type": "choice",
        "description": "Indicates the virtual SCSI or IDE controller used by the hypervisor.",
